<?xml version="1.0" encoding="UTF-8"?><web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">	<display-name>Cache Service</display-name>	<servlet>		<description>Cache Service Entry Point</description>		<display-name>TAS</display-name>		<servlet-name>cxf</servlet-name>		<servlet-class>org.apache.cxf.transport.servlet.CXFServlet</servlet-class>		<load-on-startup>1</load-on-startup>	</servlet>	<servlet-mapping>		<servlet-name>cxf</servlet-name>		<url-pattern>/cache/*</url-pattern>	</servlet-mapping>	<session-config>		<session-timeout>60</session-timeout>	</session-config>	<context-param>		<param-name>contextConfigLocation</param-name>		<param-value>classpath:cache-*-app-context.xml</param-value>	</context-param>	<listener>		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>	</listener>	</web-app>